<stax>
  
  <defaultcall function="main_virtualAttributes"/>
  
  <function name="main_virtualAttributes">
  
    <sequence>
    
      <block name="'virtualAttributes'">
        
        <sequence>
        
          <script>
            CurrentTestPath={}
            envAlreadyLoaded='true'
            CurrentTestPath['group']='virtualAttributes'
          </script>
          
          <call function="'testGroup_Preamble'"/>
          
          <import machine="STAF_LOCAL_HOSTNAME"
            file="'%s/testcases/virtualAttributes/virtualAttributes_cos.xml'
            % (TESTS_DIR)"/>
          <call function="'main_virtualAttributes_cos'" />
          
          <call function="'testGroup_Postamble'"/>
        
        </sequence>
      
      </block>
      
    </sequence>

  </function>

</stax>

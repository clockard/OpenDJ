  <defaultcall function="main_security"/>

  <function name="main_security">

      <sequence>
      
        <import machine="'%s' % (STAF_LOCAL_HOSTNAME)"
          file="'%s/testcases/security/jks/security_jks.xml' % (TESTS_DIR)"/>
